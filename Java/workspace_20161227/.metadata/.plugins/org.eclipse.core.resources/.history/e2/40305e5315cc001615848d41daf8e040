package ex2;

import java.util.Scanner;

public class TestMain {
	public static void main(String[] args) {
		
		int name;
		int phone;
		int address;
		
		Scanner sc = new Scanner(System.in);
		System.out.print("이름 : ");
		name = sc.nextInt();
		System.out.print("전화번호 : ");
		phone = sc.nextInt();
		System.out.println("주소 : ");
		address = sc.nextInt();
	}
}
