package jan2;

public class While {
   public static void main(String[] args) {
	   int i;
	   //1부터 5까지 출략하기
	   /*
	    * 초기값-조건식이 true가 되는 상황
	    * while(조건식)
	    * {
	    *  처리 ...
	    *  마지막줄에 조건식을 벗어날 수 있는 수식 
	    */
	   i=1;
	   while(i<=5)
	   {
		   System.out.println(i*3);
		   i = i+1;
	   }
	   
	   i=3;
	   while(i<=30)
	   {
		   System.out.println(i);
		   i = i+3;
	   }
 }
}
