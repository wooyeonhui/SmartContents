package jan2;

public class multiFor {
public static void main(String[] args) {
	int i, j;
	for(i=1; i<=5; i++)
	{
		for(j=1; j<=5; j++)
		{
			System.out.printf("i=%d j=%d\n",i,j);
		}
	}
	
	//구구단
	for(i=1; i<=9; i++)
	{
		System.out.printf("==%d 단==\n",i);
		for(j=1; j<=9; j++)
		{
			System.out.printf("%d X %d = %d\n",i,j,i*j);
		}
		System.out.println();
	}
	
	for(i=1; i<=5; i++)
	{
		for(j=1; j<=i; j++)
		{
			System.out.print("*");
		}
		System.out.println();
	}
	for(i=1; i<=5; i++)
	{
		for(j=5; i<=j; j--)
		{
			System.out.print("*");
		}
		System.out.println();
	}
//for 쓰고
	for(i=1; i<=25; i++)
	{
		System.out.printf("%4d",i);
		if( i%5==0)
		{
			System.out.println();			
		}
	}
//for 안쓰고
	int k=0;
	for(i=k+1; i<=k+5; i++)
	{
		for(j=0; j<5;j++)
		{
			System.out.printf("%4d\n",i+j);			
		}
		k +=5; 
	}
	int b=64;
	for(i=1; i<=5; i++)
	{
		
		for(j=1; j<=i; j++)
		{   
			System.out.print((char)(b+j));
			b++;
		}
		System.out.println();
	}
}
}
