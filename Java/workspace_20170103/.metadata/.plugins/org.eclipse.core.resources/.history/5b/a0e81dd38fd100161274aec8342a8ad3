package jan3;

public class TestMain {
public static void main(String[] args) {
	 //1. 변수 선언
	final int MAX=5;
	
	int [][]a = new int[MAX][MAX];
	int i, j, k;
	
	k=1;
	for(i=0; i<MAX-1; i++)
	{
		for(j=0; j<MAX-1; j++)
		{
			a[i][j]=k++;
		}
	}
	//2.
	
	//3.
	
	//4. 출력
	for(i=0; i<MAX; i++)
	{
		for(j=0; j<MAX; j++)
		{
			System.out.printf("%4d",a[i][j]);
		}
		System.out.println();
	}
}
}
