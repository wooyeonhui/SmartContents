package jan3;

public class Array_2D {
public static void main(String[] args) {
	int [][]arr; //배열의 배열
	
	arr = new int[3][4];//3x4가 만들어 진다.
	
	int i,j,k;
	
	k=1;
	for(i=0; i<3; i++)
	{
		for(j=0; j<4; j++)
		{
			arr[i][j] = k++;
		}
	}
	for(i=0; i<3; i++)
	{
		for(j=0; j<4; j++)
		{
			System.out.printf("%4d",arr[i][j]);
		}
		System.out.println();
	}
	
	//5X5 가로세로 합합 구하기
//	for(i=0; i<5; i++)
//	{
//		for(j=0; j<5; j++)
//		{
//			arr[i][j] = k++;
//		}
//	}
	for(i=0; i<3; i++)
	{
		for(j=0; j<4; j++)
		{
			System.out.printf("%4d",arr[i][j]);
		}
		System.out.println();
	}
	
}
}
