package jan3;

public class TestMain_2 {
	public static void main(String[] args) {
		//1. 변수 선언
	 final int MAX=10;
	 
	 int [][]a= new int[MAX][MAX];
	 int i, j, k;
	 
	 k=1;
	 //10x10
	 for(i=0; i<MAX-1; i++)
	 {
		 for(j=0; j<MAX-1; j++)
		 {
			 a[i][j]=k++;
		 }
	 }
	 //트리형
	 for(i=0; i<MAX-1; i++)
	 {
		 for(j=0; j<i; j++)
		 {
			 a[i][j]=k++;
		 }
	 }
	 //출력
	 for(i=0; i<MAX-1; i++)
	 {
		 for(j=0; j<i; j++)
		 {
			 a[i][j]=k++;
			 System.out.println(a);
		 }
		 System.out.println();
	 }
	 
	 
	}

}
