package constructor_2;

class Position {

	int x;
	int y;
	
	public Position()
	{
		x=0;
		y=0;
		System.out.println("³ª¿Í¶ó");
	}
	public Position(int n,int k)
	{
		x=n;
		y=k;
	}

	public static void main(String[] args) {
		Position ps = new Position();
		
		position ps2 = new Position(100,100);
	}

}
