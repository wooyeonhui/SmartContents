package bascketball;

import java.util.Scanner;

public class Main {
public static void main(String[] args)
{
	GameSet bascketball = new GameSet();
	bascketball.start();
      
	  Scanner sc = new Scanner(System.in);
	  //사용자 숫자 입력하기
	  int []User = new int[3]; //사용자 입력 숫자
	  for(int i=0; i<3; i++)
	  {
		  System.out.println(i+"번째 숫자 입력 : ");
		  User[i] = sc.nextInt();
	  }

}
}
