package constructor_2;

class Position {

	int x;
	int y;
	
	public Position()
	{
		x=0;
		y=0;
		System.out.println("³ª¿Í¶ó");
	}
	public Position(int n,int k)
	{
		x=n;
		y=k;
	}
	public Position(String g)
	{
		x=g;
		y=g;
	}

	public static void main(String[] args) {
		Position pos = new Position();
		
		Position pos2 = new Position(100,200);
		
		Position pos3 = new Position("100");
		
	}

}
